<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="za.co.tms.obj.Fleet" table="Fleet" optimistic-lock="version">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="alias" type="string">
            <column name="alias" length="128" />
        </property>
        <property name="idCompany" type="java.lang.Long">
            <column name="idCompany" />
        </property>
        <property name="make" type="string">
            <column name="make" length="45" />
        </property>
        <property name="model" type="string">
            <column name="model" length="45" />
        </property>
        <property name="yearModel" type="string">
            <column name="yearModel" length="45" />
        </property>
        <property name="createdTime" type="string">
            <column name="createdTime" length="45" />
        </property>
    </class>
</hibernate-mapping>